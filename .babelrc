{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            ">1% in FR"
          ]
        },
        "modules": false
      }
    ],
    "react",
    "flow"
  ],
  "plugins": [
    "transform-object-rest-spread",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ],
    "react-flow-props-to-prop-types",
    "react-hot-loader/babel"
  ]
}